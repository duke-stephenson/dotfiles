
# https://github.com/ggamel/dotfiles/issues/3
# rm /usr/local/share/zsh/site-functions/_git may work for you.

source $D/antigen/antigen/antigen.zsh
antigen use oh-my-zsh

antigen bundles <<EOBUNLDES

  adb
  brew
  # encode64
  fasd
  # httpie
  history
  git
  github
  git-hubflow
  pip
  python
  # pylint
  zsh_reload
  # systemadmin
  web-search
  # wd
  vi-mode
  virtualenvwrapper

  zsh-users/zsh-syntax-highlighting
  zsh-users/zsh-history-substring-search

EOBUNLDES

antigen bundle zsh-users/zsh-completions src
antigen apply

fpath=(~/.antigen/repos/https-COLON--SLASH--SLASH-github.com-SLASH-zsh-users-SLASH-zsh-completions.git/src/ $fpath)
